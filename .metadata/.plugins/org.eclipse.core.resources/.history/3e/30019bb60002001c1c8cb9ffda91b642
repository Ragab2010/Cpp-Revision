#include <iostream>
using namespace std;


void print(int &x){
	cout<<"int &x :"<<x<<endl;
}

void print(const int &x){
	cout<<"const int &x :"<<x<<endl;
}

//void print(const int &&x){
//	cout<<"const int &&x :"<<x<<endl;
//}




int main() {
	int x=15;
	const int y =19;
	print(x);
	print(y);
	//print(x+y);
	return 0;
}
