#include <iostream>

class Number{
private :
	int value;
public :
	Number(int value_ =0);
	Number operator+(Number &pt);
};

Number::Number(int value_){
	this->value = value_;
}

Number operator+(Number &pt){
	Number copy;
	copy.value= this->value + pt.value;
	return copy;
}



int main(){



	return 0;
}

