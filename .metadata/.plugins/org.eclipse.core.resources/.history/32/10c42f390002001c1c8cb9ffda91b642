#include <iostream>
using namespace std;

struct aa {
	int i ;
	char a[15];
};

void print(){
	cout<<"print"<<endl;
}


void print( int i ){
	cout<<"print"<<endl;
}


int main() {
	int a =10;
	int *p = &a;
	int* &pr = p;
	cout<<*pr<<endl;
	return 0;
}
