#include <iostream>
using namespace std;



//void print(int x){
//	cout<<"not template function int &x :"<<x<<endl;
//}

class Human{
public :
	int age;
	string name;
	void printAtt();

};
void Human::printAtt(){
	cout<<"name is :"<<name<<endl;
	cout<<"age  is :"<<age<<endl;

}

int main() {
	Human ahmed;
	ahmed.age=15;
	ahmed.name = "ahmed";
	ahmed.printAtt();

	return 0;
}





